{
  "name": "contract",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf ./cache-ovm ./cache ./artifacts-ovm ./artifacts ./deployments",
    "deploy-erc20-localhost": "run-s deploy-erc20-localhost:*",
    "deploy-erc20-localhost:L1ERC20": "hardhat deploy --tags L1ERC20 --network localhost_l1 --reset",
    "deploy-erc20-localhost:L2ERC20": "TARGET=ovm hardhat deploy --tags L2ERC20 --network localhost_l2 --reset",
    "deploy-erc20-localhost:L1ERC20Gateway": "hardhat deploy --tags L1ERC20Gateway --network localhost_l1 --reset",
    "deploy-erc20-localhost:initialize-L2ERC20": "hardhat run scripts/erc20/L2ERC20.initialize.ts --network localhost",
    "test-erc-20-bridge-localhost": "hardhat run scripts/erc20/test-bridge-localhost.ts --network localhost",
    "deploy-erc721-localhost": "run-s deploy-erc721-localhost:*",
    "deploy-erc721-localhost:L1ERC721": "hardhat deploy --tags L1ERC721 --network localhost_l1 --reset",
    "deploy-erc721-localhost:L2ERC721": "TARGET=ovm hardhat deploy --tags L2ERC721 --network localhost_l2 --reset",
    "deploy-erc721-localhost:L1ERC721Gateway": "hardhat deploy --tags L1ERC721Gateway --network localhost_l1 --reset",
    "deploy-erc721-localhost:initialize-L2ERC721": "hardhat run scripts/erc721/L2ERC721.initialize.ts --network localhost",
    "test-erc-721-bridge-localhost": "hardhat run scripts/erc721/test-bridge-localhost.ts --network localhost"
  },
  "dependencies": {
    "@eth-optimism/contracts": "^0.1.10",
    "@eth-optimism/plugins": "^1.0.0-alpha.2",
    "@eth-optimism/watcher": "^0.0.1-alpha.9",
    "@nomiclabs/hardhat-ethers": "^2.0.1",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "3.4.0",
    "@tenderly/hardhat-tenderly": "^1.0.11",
    "chai": "^4.2.0",
    "chalk": "^4.1.0",
    "ethereum-waffle": "^3.2.1",
    "ethers": "^5.0.24",
    "hardhat": "^2.1.2",
    "hardhat-deploy": "^0.7.0-beta.57",
    "mocha": "^8.2.1",
    "ramda": "^0.27.1"
  }
}
